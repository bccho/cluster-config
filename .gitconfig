[alias]
    com = commit -m
    stat = status
    st = stash
    stp = stash pop
    pullall = pull -p origin
    pushall = push -u origin --all
    a = add .
    needpull = fetch -v --dry-run
[push]
    default = simple
[user]
	name = Byung Cheol Cho
	email = bccho@princeton.edu
[core]
	editor = vim
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
